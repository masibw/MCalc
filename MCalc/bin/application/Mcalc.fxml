<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" style="-fx-background-color: #888;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MCalcController">
   <children>
      <HBox layoutY="360.0">
         <children>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="1" />
            <Button fx:id="button2" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="2" />
            <Button fx:id="button3" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="3" />
            <Button fx:id="buttonPercent" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="\%" />
         </children>
      </HBox>
      <HBox layoutY="420.0">
         <children>
            <Button fx:id="button4" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="4" />
            <Button fx:id="button5" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="5" />
            <Button fx:id="button6" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="6" />
            <Button fx:id="buttonMulti" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="*" />
            <Button fx:id="buttonDivide" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="/" />
         </children>
      </HBox>
      <HBox layoutY="480.0">
         <children>
            <Button fx:id="button7" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="7" />
            <Button fx:id="button8" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="8" />
            <Button fx:id="button9" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="9" />
            <Button fx:id="buttonPlus" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="+" />
            <Button fx:id="buttonMinus" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="-" />
         </children>
      </HBox>
      <HBox layoutY="540.0">
         <children>
            <Button fx:id="button0" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="0" />
            <Button fx:id="button00" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="00" />
            <Button fx:id="buttonPoint" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="." />
            <Button fx:id="buttonEqual" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="60.0" prefWidth="120.0" text="=" />
         </children>
      </HBox>
      <TextField fx:id="text_Show" alignment="CENTER" prefHeight="145.0" prefWidth="600.0" promptText="Number will be here..." />
   </children>
</AnchorPane>
